{
  "name": "vue_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "apexcharts": "^3.24.0",
    "axios": "^0.18.0",
    "google-charts": "^2.0.0",
    "moment": "^2.24.0",
    "node-gyp": "^4.0.0",
    "objects-to-csv": "^1.2.1",
    "ua-parser-js": "^0.7.19",
    "vue": "^2.6.10",
    "vue-apexcharts": "^1.3.4",
    "vue-google-charts": "^0.3.2",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.14",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@mdi/font": "^3.6.95",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^3.0.5",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^6.0.6",
    "tar": "^4.4.8",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
